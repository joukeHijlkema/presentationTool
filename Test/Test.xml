 	</table>
	<figure src="dist3.svg"
		class="top"
		style="margin-left:auto;margin-right:auto;width:70%"/>
    </slide>
    
    <!-- Overview  -->
    <slide id="overview" x="3000" y="1500" scale="5">
	<div class="hcenter vcenter">Thank you for your attention</div>
    </slide>

</presentation>
